{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"final_space": true,
	"version": 2,
	"blocks": [
		{
			"alignment": "right",
			"segments": [
				{
					"background": "#E5C07B",
					"foreground": "#ffffff",
					"leading_diamond": "\ue0b6",
					"trailing_diamond": "\ue0b4",
					"style": "diamond",
					"type": "git",
					"template": "\u279c {{ .HEAD }}",
					"properties": {
						"branch_icon": "\ufb2b "
					}
				}
			],
			"type": "rprompt"
		},
		{
			"alignment": "left",
			"segments": [
				{
					"background": "#282C34",
					"foreground": "#aaaaaa",
					"leading_diamond": "\ue0b6",
					"style": "diamond",
					"template": "{{ .UserName }}@{{ .HostName }} ",
					"type": "session"
				},
				{
					"background": "#E06C75",
					"foreground": "#ffffff",
					"powerline_symbol": "\ue0b0",
					"properties": {
						"style": "folder"
					},
					"style": "powerline",
					"template": " {{ .Path }} ",
					"type": "path"
				},
				{
					"foreground": "#ABB2BF",
					"style": "plain",
					"type": "text",
					"template": " >"
				}
			],
			"type": "prompt"
		}
	],
	"final_space": true,
	"version": 2
}
